{
  "name": "@storybook/design-system",
  "version": "2.0.1",
  "description": "Storybook design system",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/design-system.git"
  },
  "license": "MIT",
  "author": {
    "name": "Michael Shilman",
    "email": "michael@lab80.co"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "build-docs": "build-storybook --docs",
    "build-storybook": "build-storybook",
    "chromatic": "chromatic --appCode=\"9ofhn0iql7n\"",
    "chromatic-static": "chromatic -d=\"./storybook-static\" --appCode=\"9ofhn0iql7n\" --debug",
    "lint": "eslint .",
    "lint:js": "cross-env NODE_ENV=production eslint --cache --cache-location=.cache/eslint --ext .js,.jsx,.html,.ts,.tsx,.mjs --report-unused-disable-directives",
    "lint:package": "sort-package-json",
    "release": "npm run build && ./scripts/release.sh",
    "storybook": "start-storybook -p 6006"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.html": [
      "yarn lint:js --fix",
      "git add"
    ],
    "*.js": [
      "yarn lint:js --fix",
      "git add"
    ],
    "package.json": [
      "yarn lint:package",
      "git add"
    ]
  },
  "dependencies": {
    "@storybook/components": "^5.3.0-beta.16",
    "@storybook/theming": "^5.3.0-beta.16",
    "keycode": "^2.2.0",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react-modal": "^3.11.1",
    "react-popper-tooltip": "^2.10.1",
    "recompose": "^0.30.0",
    "styled-components": "^4.4.1",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@storybook/addon-a11y": "^5.3.0-beta.16",
    "@storybook/addon-actions": "^5.3.0-beta.16",
    "@storybook/addon-docs": "^5.3.0-beta.16",
    "@storybook/addon-knobs": "^5.3.0-beta.16",
    "@storybook/addon-storysource": "^5.3.0-beta.16",
    "@storybook/addons": "^5.3.0-beta.16",
    "@storybook/eslint-config-storybook": "^2.1.0",
    "@storybook/react": "^5.3.0-beta.16",
    "@storybook/source-loader": "^5.3.0-beta.16",
    "auto": "^7.16.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-require-context-hook": "^1.0.0",
    "babel-plugin-styled-components": "^1.10.6",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "remark": "^11.0.2",
    "sort-package-json": "^1.30.0",
    "storybook-chromatic": "^3.4.1",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "auto": {
    "plugins": [
      "npm",
      "released"
    ]
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/storybook"
  }
}
